{
  "functions": {
    "api/*.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.py"
    }
  ],
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "build": {
    "env": {
      "PYTHONPATH": "/var/task/web:/var/task",
      "PIP_CUSTOM_PATH": "web/requirements.txt"
    }
  }
}
